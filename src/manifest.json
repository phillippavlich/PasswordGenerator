{
  "name": "PretzelPass",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Password generator with extra salt!",
  "homepage_url": "https://gitlab.cas.mcmaster.ca/mullentc/3xa3-group-project",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "PretzelPass",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "clipboardWrite",
    "tabs",
    "downloads"
  ]
}